{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc --noEmit && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "compile": "hardhat compile --config hardhat.config.cjs",
    "compile:size": "hardhat compile --config hardhat.config.cjs && hardhat size-contracts --config hardhat.config.cjs",
    "indexer": "USE_GIFTERRA=true tsx src/indexer/index.ts",
    "indexer:backfill": "ENABLE_BACKFILL=true USE_GIFTERRA=true tsx src/indexer/index.ts",
    "build:mobile": "vite build && npx cap sync",
    "sync": "npx cap sync",
    "sync:android": "npx cap sync android",
    "sync:ios": "npx cap sync ios",
    "open:android": "npx cap open android",
    "open:ios": "npx cap open ios",
    "copy": "npx cap copy",
    "copy:android": "npx cap copy android",
    "copy:ios": "npx cap copy ios"
  },
  "dependencies": {
    "@capacitor-community/barcode-scanner": "^4.0.1",
    "@capacitor/android": "^7.4.4",
    "@capacitor/camera": "^7.0.2",
    "@capacitor/cli": "^7.4.4",
    "@capacitor/core": "^7.4.4",
    "@capacitor/haptics": "^7.0.2",
    "@capacitor/ios": "^7.4.4",
    "@capacitor/keyboard": "^7.0.3",
    "@capacitor/motion": "^7.0.1",
    "@capacitor/status-bar": "^7.0.3",
    "@gelatonetwork/relay-sdk": "^5.6.0",
    "@gelatonetwork/smartwallet": "0.1.0-alpha.6",
    "@privy-io/react-auth": "^3.4.1",
    "@stripe/stripe-js": "^8.5.3",
    "@supabase/supabase-js": "^2.86.0",
    "@thirdweb-dev/chains": "^0.1.120",
    "@thirdweb-dev/react": "^4.9.4",
    "@thirdweb-dev/sdk": "^4.0.99",
    "@thirdweb-dev/wallets": "^2.5.39",
    "@types/qrcode": "^1.5.6",
    "@web3modal/wagmi": "^5.1.11",
    "@zxing/browser": "^0.1.5",
    "@zxing/library": "^0.21.3",
    "buffer": "^6.0.3",
    "canvas-confetti": "^1.9.3",
    "dotenv": "^17.2.3",
    "ethers": "^5.8.0",
    "jszip": "^3.10.1",
    "permissionless": "^0.2.57",
    "qrcode": "^1.5.4",
    "qrcode.react": "^4.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "recharts": "^3.2.1",
    "stripe": "^20.0.0",
    "web-push": "^3.6.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@nomicfoundation/hardhat-toolbox": "^2.0.2",
    "@nomiclabs/hardhat-ethers": "^2.2.3",
    "@openzeppelin/contracts": "^4.9.6",
    "@typechain/ethers-v5": "^10.2.1",
    "@typechain/hardhat": "^6.1.6",
    "@types/canvas-confetti": "^1.9.0",
    "@types/jszip": "^3.4.1",
    "@types/node": "^24.6.0",
    "@types/react": "^18.3.25",
    "@types/react-dom": "^18.3.7",
    "@types/web-push": "^3.6.4",
    "@vercel/node": "^5.4.0",
    "@vitejs/plugin-react": "^5.0.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "hardhat": "^2.26.3",
    "hardhat-contract-sizer": "^2.10.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.7",
    "vite-plugin-pwa": "^1.1.0"
  }
}
